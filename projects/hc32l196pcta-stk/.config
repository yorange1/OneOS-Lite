CONFIG_BOARD_HC32L1=y
CONFIG_ARCH_ARM=y
CONFIG_ARCH_ARM_CORTEX_M=y
CONFIG_ARCH_ARM_CORTEX_M0=y

#
# Kernel
#
CONFIG_OS_MINIMAL_CONFIG=y
# CONFIG_OS_CUSTOMIZED_CONFIG is not set
CONFIG_OS_NAME_MAX=0
CONFIG_OS_TASK_PRIORITY_MAX=8
CONFIG_OS_MAIN_TASK_STACK_SIZE=1300
CONFIG_OS_IDLE_TASK_STACK_SIZE=128
CONFIG_OS_RECYCLE_TASK_STACK_SIZE=128
CONFIG_OS_USING_TIMER=y
CONFIG_OS_TIMER_TASK_STACK_SIZE=256
CONFIG_OS_USING_SEMAPHORE=y
CONFIG_OS_USING_MESSAGEQUEUE=y
CONFIG_OS_USING_SYS_HEAP=y
CONFIG_OS_USING_MEM_HEAP=y
CONFIG_OS_USING_ALG_SMALL=y
CONFIG_OS_MEM_ALGORITHM_MINIMIZATION=y
CONFIG_OS_DEBUG=y
CONFIG_EXCEPTION_SIMPLE_HANDLE=y
CONFIG_OS_TICK_PER_SECOND=100
CONFIG_OS_SCHEDULE_TIME_SLICE=10
# end of Kernel

#
# Drivers
#

#
# Device
#
CONFIG_OS_USING_DEVICE=y
CONFIG_OS_DEVICE_NAME_MAX=15
# CONFIG_OS_USING_DEVICE_BLOCK is not set
CONFIG_OS_USING_DRIVER_SIMPLE=y
# end of Device

CONFIG_OS_USING_FAL=y

#
# Console
#
CONFIG_OS_USING_CONSOLE=y
CONFIG_OS_CONSOLE_DEVICE_NAME="uart1"
CONFIG_OS_LOG_BUFF_SIZE=256
# end of Console

#
# Serial
#
CONFIG_OS_USING_SERIAL=y
CONFIG_OS_SERIAL_IDLE_TIMER=y
# CONFIG_OS_SERIAL_DELAY_CLOSE is not set
CONFIG_OS_SERIAL_RX_BUFSZ=512
CONFIG_OS_SERIAL_TX_BUFSZ=64
# end of Serial

#
# Timer
#
# CONFIG_OS_USING_TIMER_DRIVER is not set
# end of Timer

#
# LPMGR
#
# CONFIG_OS_USING_LPMGR is not set
# end of LPMGR

CONFIG_OS_USING_PUSH_BUTTON=y
CONFIG_OS_USING_LED=y

#
# ADC
#
# CONFIG_OS_USING_ADC is not set
# end of ADC

CONFIG_OS_USING_PIN=y
CONFIG_OS_PIN_MAX_CHIP=1

#
# HAL
#
CONFIG_MANUFACTOR_HC32=y
CONFIG_SERIES_HC32L19=y
CONFIG_SOC_HC32L19X=y
CONFIG_BSP_BOOT_OPTION=y
CONFIG_BSP_TEXT_SECTION_ADDR=0x00000000
CONFIG_BSP_TEXT_SECTION_SIZE=0x00040000
CONFIG_BSP_DATA_SECTION_ADDR=0x20000000
CONFIG_BSP_DATA_SECTION_SIZE=0x00008000
CONFIG_BSP_USING_GPIO=y
CONFIG_BSP_USING_UART=y
# CONFIG_BSP_USING_LPUART0 is not set
# CONFIG_BSP_USING_LPUART1 is not set
# CONFIG_BSP_USING_UART0 is not set
CONFIG_BSP_USING_UART1=y
# CONFIG_BSP_USING_UART2 is not set
# CONFIG_BSP_USING_UART3 is not set
CONFIG_BSP_USING_ON_CHIP_FLASH=y
# end of HAL

#
# RTC
#
# CONFIG_OS_USING_RTC is not set
# end of RTC
# end of Drivers

#
# Components
#
CONFIG_OS_USING_NEWLIB_ADAPTER=y
CONFIG_OS_USING_ARMCCLIB_ADAPTER=y
CONFIG_OS_USING_LIBC_ADAPTER=y

#
# Molink
#
# CONFIG_NET_USING_MOLINK is not set
# end of Molink

#
# OTA
#

#
# CMIOT Fota
#
# CONFIG_FOTA_USING_CMIOT is not set
# end of CMIOT Fota
# end of OTA

#
# Shell
#
# CONFIG_OS_USING_SHELL is not set
# end of Shell

#
# Socket
#
# CONFIG_NET_USING_BSD is not set
# end of Socket

#
# Cplusplus
#
# CONFIG_OS_USING_CPLUSPLUS is not set
# end of Cplusplus
# end of Components

#
# Thirdparty
#

#
# Submodule
#
CONFIG_OS_USING_SUBMODULE=y
# end of Submodule
# end of Thirdparty
